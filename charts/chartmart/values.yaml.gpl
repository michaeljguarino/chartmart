postgresql:
  postgresqlPassword: {{ randAlphaNum 32 }}

secrets:
  jwt: {{ randAlphaNum 32 }}
  erlang: {{ randAlphaNum 32 }}

admin:
  enabled: true
  name: {{ .Values.admin_name }}
  email: {{ .Values.admin_email }}
  password: {{ randAlphaNum 12 }}
  publisher: {{ .Values.publisher }}
  publisher_description: {{ .Values.publisher_description }}

ingress:
  dns: {{ .Values.chartmart_dns }}

api:
  bucket: {{ .Values.assets_bucket }}

chartmuseum:
  bucket: {{ .Values.chartmuseum_bucket }}
